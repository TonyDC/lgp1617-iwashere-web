{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "env": {
    // Hot Module Replacement
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    // For Webpack 2 Code splitting feature import() to be supported by Jest
    "test": {
      "plugins": [
        "babel-plugin-dynamic-import-node"
      ]
    }
  }
}
